<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="kim" class="com.test02.Address">
		<constructor-arg name="name" value="김미림"/>
		<constructor-arg name="addr" value="서울시"/>
		<constructor-arg name="phone" value="010-1111-1111"/>
	</bean>
	
	<!-- name을 지정해줌으로 mapping처리를 해줄 수 있다. -->
	<bean id="ko" class="com.test02.Address">
		<constructor-arg name="name" value="고한솔"/>
		<constructor-arg name="addr" value="경기도"/>
		<constructor-arg name="phone" value="010-2222-2222"/>
	</bean>
</beans>
