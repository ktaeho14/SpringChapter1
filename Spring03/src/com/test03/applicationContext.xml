<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hong" class="com.test03.Developer">
		<constructor-arg index="0" value="홍길동"/>
		<constructor-arg index="1" value="2500000"/>
		<property name="dept" value="개발"/>
		
	</bean>
	
	<bean id="kim" class="com.test03.Engineer">
		<constructor-arg index="0" value="김유신"/>
		<constructor-arg index="1" value="2000000"/>
		<property name="dept" value="기술"/>
	</bean>
</beans>
